{
   "name": "cotacao-crypto-api",
   "version": "1.1.0",
   "description": "API de Cotação de Criptomoedas",
   "main": "build/src/server.js",
   "license": "MIT",
   "scripts": {
      "clear": "rm -rf build",
      "prebuild": "npm run swagger",
      "build": "tsc",
      "start": "ts-node src/server.ts",
      "predev": "npm run swagger",
      "dev": "concurrently \"ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts\" \"tsoa spec-and-routes\"",
      "swagger": "tsoa spec-and-routes",
      "test": "jest",
      "typeorm": "typeorm-ts-node-commonjs"
   },
   "dependencies": {
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "express-async-errors": "^3.1.1",
      "morgan": "^1.10.0",
      "multer": "^1.4.5-lts.1",
      "mysql2": "^2.3.3",
      "phoenix-channels": "^1.0.0",
      "reflect-metadata": "^0.1.13",
      "swagger-ui-express": "^4.5.0",
      "tsoa": "^4.1.3",
      "typeorm": "0.3.11",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.14",
      "@types/jest": "^29.2.0",
      "@types/morgan": "^1.9.3",
      "@types/multer": "^1.4.7",
      "@types/node": "^16.11.10",
      "@types/supertest": "^2.0.12",
      "@types/swagger-ui-express": "^4.1.3",
      "@types/uuid": "^8.3.4",
      "@typescript-eslint/eslint-plugin": "^5.0.0",
      "concurrently": "^7.5.0",
      "dotenv": "^16.0.3",
      "eslint": "^8.0.1",
      "eslint-config-prettier": "^8.5.0",
      "eslint-config-standard-with-typescript": "^23.0.0",
      "eslint-import-resolver-typescript": "^3.5.1",
      "eslint-plugin-import": "^2.25.2",
      "eslint-plugin-n": "^15.0.0",
      "eslint-plugin-prettier": "^4.2.1",
      "eslint-plugin-promise": "^6.0.0",
      "jest": "^29.2.2",
      "prettier": "^2.7.1",
      "supertest": "^6.3.1",
      "ts-jest": "^29.0.3",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "4.5.2"
   }
